{
  "expo": {
    "name": "Feud Fusion: What Would They Say",
    "slug": "feudfusionwhatwouldtheysay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "feudfusion",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.feudfusionwhatwouldtheysay.app",
      "usesGameCenter": true,
      "entitlements": {
        "com.apple.developer.game-center": true
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "GCSupportsGameCenterLogin": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0A0E27",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.feudfusionwhatwouldtheysay.app",
      "versionCode": 5,
      "permissions": [
        "INTERNET",
        "VIBRATE"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0A0E27",
          "dark": {
            "backgroundColor": "#0A0E27"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}